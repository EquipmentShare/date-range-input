{
  "name": "@equipmentshare/date-range-input",
  "description": "A user interface for inputting date ranges",
  "version": "1.0.2",
  "engines": {
    "node": ">=13"
  },
  "type": "module",
  "main": "./bundle.js",
  "svelte": "src/DateRangeInput.svelte",
  "homepage": "https://gitlab.internal.equipmentshare.com/time-tracking/date-range-input",
  "repository": {
    "type": "git",
    "url": "git@gitlab.internal.equipmentshare.com:time-tracking/date-range-input.git"
  },
  "scripts": {
    "pre-build": "foundation --targetDirectory=src/css",
    "build": "npm-run-all --print-label pre-build --parallel build:*",
    "build:js": "rollup -c",
    "build-dev": "cross-env ENVIRONMENT=dev npm run build",
    "dev:serve": "live-server public",
    "dev:watch:js": "npm run build:js -- -w",
    "dev": "npm-run-all --print-label pre-build --parallel dev:**",
    "autoformat": "eslint rollup.config.js src/ --fix --ext .js,.svelte",
    "lint": "eslint rollup.config.js src/ --ext .js,.svelte",
    "test": "glob-module-file --pattern=\"src/**/*.test.js\" --importStar --format=es | node --input-type=module"
  },
  "devDependencies": {
    "@equipmentshare/es-foundation": "^5.0.1",
    "@equipmentshare/eslint-config-es-base-js": "^1.0.0",
    "calendarize": "^1.1.0",
    "click-should-be-intercepted-for-navigation": "^1.0.0",
    "cpy-cli": "^2.0.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "glob-module-file": "^3.0.1",
    "live-server": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^12.0.1",
    "rollup": "^1.12.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.0.3",
    "svelte": "^3.0.0",
    "svelte-preprocess-postcss": "^1.1.1",
    "zora": "^3.1.8"
  },
  "dependencies": {}
}
